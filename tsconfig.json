{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["ES2020", "DOM", "DOM.Iterable"],
    "module": "ESNext",
    "skipLibCheck": true,
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "strict": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "jsx": "react-jsx"
  },
  "include": ["src1", "vite.config.ts"],
  "exclude": ["node_modules", "src1/lib/supabase", "deno-relay.ts", "deno-relay-*.ts", "*.mjs", "*.js"]
}
